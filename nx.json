{
  "npmScope": "ultimate-backend",
  "affected": {
    "defaultBase": "next"
  },
  "workspaceLayout": {
    "appsDir": "examples",
    "libsDir": "packages"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*",
      "peerDependencies": "*",
      "scripts": {
        "check:*": "*"
      }
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "accessToken": "MDVhYjA4NTUtZDY0YS00MzhlLWI1YTgtNjE2MjVmYWNmNGQxfHJlYWQtd3JpdGU=",
        "cacheableOperations": ["build", "test", "lint", "e2e"],
        "canTrackAnalytics": false,
        "showUsageWarnings": true,
        "parallel": 1
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "cli",
  "generators": {
    "@nrwl/node:library": {
      "buildable": true,
      "publishable": true
    }
  }
}
